{
    "rewrites":  [
      {"source": "/(.*)", "destination": "/"}
    ],
    "headers": [
      {
        "source": "/",
        "has": [
          {
            "type": "host",
            "value": "example.com"
          }
        ],
        "headers" : [
          {
            "key" : "X-Robots-Tag",
            "value" : "index"
          }
        ]
      }
    ]
  }
  